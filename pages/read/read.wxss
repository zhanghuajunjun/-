.title {
  font-size: 40rpx;
  font-weight: 600;
  padding: 20rpx 30rpx;
  border-bottom: 1rpx solid #ccc;
}

.body {
  padding: 20rpx 30rpx;
}

.item {
  font-size: 34rpx;
  font-weight: 300;
  text-indent: 40rpx;
  text-align: left;
  line-height: 80rpx;
  color: white;
}

.box {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.top {
  padding: 10rpx 30rpx;
  color: white;
  background: red;
  border-radius: 40rpx 0 0 40rpx;
  margin-right: 6rpx;
  width: 100rpx;
}

.bottom {
  padding: 10rpx 30rpx;
  color: white;
  background: red;
  border-radius: 0 40rpx 40rpx 0;
  width: 100rpx;
}

.zhang {
  width: 100%;
  display: flex;
  justify-content: center;
  position: absolute;
  bottom: 50rpx;
  z-index: 66;
}

.Uzi {
  position: absolute;
  bottom: 0;
  background: rgba(0, 0, 0, .8);
  width: 100%;
  z-index: 99;
}

.see {
  padding: 32rpx 0;
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: white;
}

.backg {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}

.background {
  padding: 20rpx 30rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
  color: white;
}

.catalog {
  height: 1207rpx;
  width: 300rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.catalog-title {
  padding: 26rpx;
  font-size: 32rpx;
  font-weight: 600;
  text-align: center;
  border-bottom: 1rpx solid #ccc;
}

.catalog-item {
  padding: 20rpx;
  font-size: 30rpx;
  text-align: center;
}